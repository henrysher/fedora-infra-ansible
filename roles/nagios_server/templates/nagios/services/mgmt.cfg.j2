# HTTP

{% for host in vars['phx2_management_hosts'] %}
define service {
  host_name             {{ host }}
  service_description   {{ host }}-http
  check_command         check_http!{{ host }}
  use                   defaulttemplate
}

define service {
  host_name             {{ host }}
  service_description   {{ host }}-https
  check_command         check_https!{{ host }}
  use                   defaulttemplate
}


{% endfor %}

