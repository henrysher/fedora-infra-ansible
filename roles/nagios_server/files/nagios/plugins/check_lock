#!/usr/bin/python2

import fcntl
import sys

try:
    f = open('/mnt/koji/.nagios_test', 'r')
    f.close()
    f = open('/mnt/koji/.nagios_test', 'w')
except IOError:
    print "Could not create file"
    sys.exit(2)
    
fcntl.flock(f, fcntl.LOCK_EX)
f.close()
print "File Locked Successfully"
sys.exit(0)
